{
  "name": "ec-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4001",
    "build": "next build",
    "start": "next start",
    "apollo:download-schema": "apollo client:download-schema schema.graphql",
    "apollo:generate-types": "rm -rf __generated__ && apollo client:codegen --target typescript --outputFlat",
    "apollo:download": "apollo service:download --endpoint=http://localhost:4000/ graphql-schema.json",
    "apollo:sync": "npm run apollo:download-schema && npm run apollo:generate-types",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "jest -c test/jest.config.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:test": "start-server-and-test start http://localhost:4001 cypress:run",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.3.18",
    "@reduxjs/toolkit": "^1.5.1",
    "crypto-hash": "^1.3.0",
    "graphql": "^15.5.0",
    "next": "^10.2.0",
    "next-i18next": "^8.3.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-use": "^17.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "apollo": "^2.33.2",
    "autoprefixer": "^10.2.5",
    "cypress": "^7.3.0",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "nock": "^13.0.11",
    "postcss": "^8.2.15",
    "start-server-and-test": "^1.12.2",
    "tailwindcss": "^2.1.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  }
}
